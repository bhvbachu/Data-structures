//function overloading
#include<iostream>
using namespace std;
class math{
    public:
    int add(int a,int b){
        return a+b;
    }
    float add(float a,float b){
   return a+b;
    }
    int add(int a,int b,int c){
        return a+b+c;
    }
};
int main(){
    math m;
    cout<<"add of two integers: "<<m.add(10,20)<<endl;
 cout<<"add of two integers: "<<m.add(2.5f,4.5f)<<endl;
 cout<<"add of three integers: "<<m.add(10,20,10)<<endl;
 return 0;   
}


